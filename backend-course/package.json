    "express-validator": "^4.2.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "multer": "^1.3.0",
    "mysql2": "^1.5.1",
    "nodemailer": "^4.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.22.15",
    "socket.io": "^2.0.3",
    "validator": "^9.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
