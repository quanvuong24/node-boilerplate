  "scripts": {
    "eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run eslint -- . ",
    "lint": "npm run lint:js",
    "seed": "cross-env NODE_ENV=development babel-node src/seeds/migrate.js",
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "production": "cross-env NODE_ENV=production nodemon --exec babel-node src/index.js",
    "clean": "rimraf build",
    "dev:build": "npm run clean && webpack -w",
    "dev": "cross-env NODE_ENV=development nodemon build/app.bundle.js"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "crypto-js": "^3.1.9-1",
